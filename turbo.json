{
    "$schema": "https://turbo.build/schema.json",
    "globalDependencies": [
        ".env"
    ],
    "globalEnv": [
        "PRIVATE_KEY",
        "INFURA_ID",
        "NEXT_PUBLIC_INFURA_ID",
        "REPORT_GAS",
        "NODE_ENV",
        "PORT",
        "VERCEL_URL",
        "SUPABASE_STORAGE_URL",
        "SUPABASE_SERVICE_KEY",
        "DATABASE_URL",
        "NEXTAUTH_URL",
        "NEXTAUTH_SECRET",
        "WEBAPP_BASE_URL",
        "REDIS_URL",
        "EVENTS_SERVER_URL",
        "EVENTS_SERVER_AUTH_TOKEN",
        "CI",
        "WIDGET_BASE_URL"
    ],
    "pipeline": {
        "build": {
            "dependsOn": [
                "generate",
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "test": {
            "inputs": [
                "src/**/*.tsx",
                "src/**/*.ts",
                "test/**/*.ts",
                "test/**/*.tsx"
            ],
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "dependsOn": [
                "generate"
            ],
            "cache": false
        },
        "clean": {
            "cache": false
        },
        "generate": {
            "dependsOn": [
                "^generate"
            ]
        },
        "format:check": {
            "outputs": []
        }
    }
}