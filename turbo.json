{
    "$schema": "https://turbo.build/schema.json",
    "globalDotEnv": [
        ".env"
    ],
    "globalEnv": [
        "REPORT_GAS",
        "NODE_ENV",
        "PORT",
        "NEXT_PUBLIC_VERCEL_ENV",
        "NEXT_PUBLIC_VERCEL_URL",
        "CI"
    ],
    "pipeline": {
        "build": {
            "dependsOn": [
                "generate",
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "test": {
            "inputs": [
                "src/**/*.tsx",
                "src/**/*.ts",
                "test/**/*.ts",
                "test/**/*.tsx"
            ],
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "dependsOn": [
                "generate"
            ],
            "cache": false
        },
        "start": {
            "cache": false
        },
        "clean": {
            "cache": false
        },
        "generate": {
            "dependsOn": [
                "^generate"
            ]
        },
        "format:check": {
            "outputs": []
        }
    }
}