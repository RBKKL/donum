{
  "name": "@donum/prisma",
  "private": true,
  "scripts": {
    "clean": "rimraf .turbo node_modules",
    "with-env": "dotenv -e ../../.env",
    "dev": "pnpm with-env prisma studio",
    "generate": "pnpm with-env prisma db push",
    "seed": "pnpm with-env prisma db seed",
    "postinstall": "pnpm with-env prisma generate && pnpm seed"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@types/node": "18.11.9",
    "prisma": "^4.6.0",
    "ts-node": "^10.9.1",
    "typescript": "4.9.3"
  },
  "prisma": {
    "seed": "ts-node --transpile-only ./seed.ts"
  }
}